{
  "name": "vue3-aplayer",
  "version": "1.7.3",
  "description": "Easy-to-use music player for Vue 2.x",
  "main": "dist/vue-aplayer.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack-dev-server --hot-only --progress --color --host 0.0.0.0 --port 3000 --config webpack.demo.config.js",
    "build": "export NODE_ENV=production && webpack --progress",
    "build:demo": "export NODE_ENV=production && webpack --progress --config webpack.demo.config.js",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --ext .js,.vue src",
    "test": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/aplayer3.git"
  },
  "keywords": [
    "vue",
    "aplayer",
    "vue-aplayer",
    "music-player",
    "html5",
    "component",
    "vue-compoents"
  ],
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/aplayer3/issues"
  },
  "homepage": "https://github.com/Mister-Hope/aplayer3#readme",
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "3.2.47",
    "autoprefixer": "10.4.14",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.10.0",
    "hls.js": "1.4.0",
    "prettier": "2.8.7",
    "vite": "4.2.1",
    "vue": "3.2.47"
  },
  "browserslist": [
    "> 0.5%",
    "last 5 versions",
    "not ie"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
